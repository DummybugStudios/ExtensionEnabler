{
  "manifest_version":3,
  "name":"Extension Manager",
  "description":"Extension that manages other extensions",
  "version":"0.0.0.2",

  "action":{
    "default_popup":"popup.html",
    "default_script":"popup.js"
  },
  "permissions":[
    "management"
  ],
  "background":{
      "service_worker":"background.js"
  },
  "icons" : {
    "16":"icon16.png",
    "48":"icon48.png",
    "128":"icon128.png"
  },
  "commands": {
    "_execute_browser_action": {
       "suggested_key": {
         "default": "Ctrl+L"
       }
    }
  }

}
