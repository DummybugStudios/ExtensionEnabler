{
  "manifest_version":2,
  "name":"Extension Manager",
  "description":"Extension that manages other extensions",
  "version":"0.0.0.1",

  "browser_action":{
    "default_popup":"popup.html",
    "default_script":"popup.js"
  },
  "permissions":[
    "activeTab",
    "management"
  ],
  "background":{
      "scripts":["background.js"]
  }
}
